<%= render "shared/dashboard" %>

<div style="margin-left:25%;padding:86px 28px;height:1000px;">
  <h2>Remove Teammembers</h2>
    <%= simple_form_for([@user, @team]) do |f| %>
    <%= f.association :users, label: "Who do you want to remove from the team:", label_method: :user_name, method: :delete %>
    <%= f.submit %>
    <% end %>
</div>